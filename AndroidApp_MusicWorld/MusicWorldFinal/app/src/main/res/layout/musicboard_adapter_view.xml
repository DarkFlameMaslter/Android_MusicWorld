<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="15dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.cardview.widget.CardView
        android:id="@+id/MusicBoard_cardView"
        android:layout_centerVertical="true"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:cardCornerRadius="5dp">

        <ImageView
            android:id="@+id/icon_view"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/hololivemusic" />
    </androidx.cardview.widget.CardView>



    <TextView
        android:id="@+id/MusicBoard_TitleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_centerVertical="true"
        android:lines="1"
        android:textSize="20dp"
        android:fontFamily="@font/mto_chancery"
        android:textColor="@color/deep_sea_blue"
        android:background="@color/white"
        android:layout_toEndOf="@+id/MusicBoard_cardView"
        android:text="TextView" />
</RelativeLayout>